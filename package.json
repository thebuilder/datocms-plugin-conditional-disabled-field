{
  "name": "datocms-plugin-conditional-disabled-field",
  "description": "A plugin for DatoCMS that allows you disable fields conditionally.",
  "version": "2.0.0",
  "license": "MIT",
  "homepage": "https://github.com/thebuilder/datocms-plugin-conditional-disabled-field#readme",
  "author": "Daniel Schmidt",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "docs",
    "dist"
  ],
  "main": "dist/index.html",
  "repository": {
